<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShadowLayout">
        <attr name="offset" format="dimension"/>
        <attr name="dx" format="dimension"/>
        <attr name="dy" format="dimension"/>
        <attr name="rx" format="dimension"/>
        <attr name="ry" format="dimension"/>
        <attr name="shadowRadius" format="dimension"/>
        <attr name="fillColor" format="color"/>
        <attr name="shadowColor" format="color"/>
        <attr name="hole" format="boolean"/>
        <attr name="type" format="enum">
            <enum name="roundRect" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>
</resources>